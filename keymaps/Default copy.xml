<keymap version="1" name="Default copy" parent="$default">
  <action id="GotoDeclaration">
    <mouse-shortcut keystroke="control button1" />
    <mouse-shortcut keystroke="button2" />
  </action>
  <action id="PlaybackLastMacro">
    <keyboard-shortcut first-keystroke="ctrl b" />
  </action>
  <action id="ReopenClosedTab">
    <keyboard-shortcut first-keystroke="ctrl t" />
  </action>
  <action id="UnderScoreToggleToCamel">
    <keyboard-shortcut first-keystroke="shift ctrl f3" />
    <keyboard-shortcut first-keystroke="shift ctrl q" />
  </action>
  <action id="Vcs.UpdateProject" />
  <action id="de.netnexus.CamelCasePlugin.ToggleCamelCase">
    <keyboard-shortcut first-keystroke="shift alt u" />
    <keyboard-shortcut first-keystroke="shift ctrl q" />
  </action>
</keymap>